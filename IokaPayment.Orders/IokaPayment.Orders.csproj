        <PackageTags>Ioka</PackageTags>
        <PackageReleaseNotes>Change .NET version to .NET 7.0</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\IokaPayment.General\IokaPayment.General.csproj" />
    </ItemGroup>

</Project>
